/* Memories definition */
MEMORY
{
  ROM    (xrw)    : ORIGIN = 0x34180400,   LENGTH = 255K
  RAM    (xrw)    : ORIGIN = 0x341C0000,   LENGTH = 256K
  
  RAM_BIG (xrw)   : ORIGIN = 0x24100000, LENGTH = 300K
}

/* Sections */
SECTIONS
{   
  .big_data :
  {
	  . = ALIGN(32);
	  __big_data_start__ = .;
	  *(.big_data)
	  *(.big_data.*)
	  . = ALIGN(32);
	  __big_data_end__ = .;
  } > RAM_BIG
}
